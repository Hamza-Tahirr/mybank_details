    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <App id="app">
        <Page id="page" title="My Bank Details">
            <content>

                <core:Fragment id="bankDetailsfragment" fragmentName="com.sap.mybankdetails.view.fragments.BankDetails" type="XML"/>

                <Button id="openBankDetails" type="Emphasized" text="Find more Details.."></Button>

            </content>

        </Page>
    </App>
</mvc:View>
